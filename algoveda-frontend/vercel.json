{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "http://localhost:5001/api/$1"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    }
  ]
}